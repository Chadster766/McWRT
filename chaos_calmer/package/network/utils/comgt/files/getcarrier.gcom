opengt
  set senddelay 0.05
  waitquiet 1 0.2
  let c=1
 :loop
    inc c
    send "AT+CGATT?^m"
    waitfor 5 "+CGATT: 1","+CGATT: 0"
    print "\n."
    if % = -1 goto error
    if c > 10 goto toolong
    if % = 0 goto out
    sleep 2
    if % = 1 goto loop
  :toolong
  exit 1
  :error
  exit 0
  :out
 exit 0
