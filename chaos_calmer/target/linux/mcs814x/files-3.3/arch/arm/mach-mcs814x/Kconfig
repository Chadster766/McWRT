if ARCH_MCS814X

config MCS8140
	bool
	select CPU_ARM926T

menu "Moschip MCS8140 boards"

config MACH_DLAN_USB_EXT
	bool "Devolo dLAN USB Extender"
	select MCS8140
	select NEW_LEDS
	select LEDS_CLASS
	select LEDS_GPIO
	help
	  Machine support for the Devolo dLAN USB Extender

config MACH_RBT_832
	bool "Tigal RBT-832"
	select MCS8140
	select NEW_LEDS
	select LEDS_CLASS
	select LEDS_GPIO
	help
	  Machine support for the Tigal RBT-832 board

endmenu

endif
