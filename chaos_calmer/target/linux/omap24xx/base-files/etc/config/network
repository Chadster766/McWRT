config interface loopback
	option ifname	lo
	option proto	static
	option netmask	255.0.0.0

config interface lan
	option proto	dhcp
	option netmask	255.255.255.0

config interface usb
	option ifname	usb0
	option proto	static
	option ipaddr	192.168.168.168
	option netmask	255.255.255.0
	option ip6addr	fe80::810:1
