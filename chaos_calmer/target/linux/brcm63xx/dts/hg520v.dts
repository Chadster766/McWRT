/dts-v1/;

/include/ "bcm6358.dtsi"

/ {
	model = "Huawei EchoLife HG520v";
	compatible = "huawei,hg520v", "brcm,bcm6358";
};

&pflash {
	status = "ok";

	linux,part-probe = "bcm63xxpart";

	cfe@0 {
		label = "CFE";
		reg = <0x000000 0x010000>;
		read-only;
	};

	linux@10000 {
		label = "linux";
		reg = <0x010000 0x3e0000>;
	};

	nvram@3f0000 {
		label = "nvram";
		reg = <0x3f0000 0x010000>;
	};
};
