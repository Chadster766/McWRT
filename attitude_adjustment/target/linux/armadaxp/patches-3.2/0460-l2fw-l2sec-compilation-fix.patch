 	mvNetaTxqPendDescAdd(pp->port, pp->txp, txq, 1);
 
-	mv_eth_unlock(txq_ctrl, pp);
+	mv_eth_unlock(txq_ctrl, flags);
 
 	read_unlock(&pp->rwlock);
 
