From fae942b3f56d47e5f8dd46514ea84c1ec13dda9f Mon Sep 17 00:00:00 2001
From: Seif Mazareeb <seif@marvell.com>
Date: Sun, 24 Jun 2012 16:36:58 +0300
Subject: [PATCH 167/609] enabling ARMADA_XP_REV_A0 as default when enabling
 "Armada XP Chip revision"

Signed-off-by: Seif Mazareeb <seif@marvell.com>
---
 arch/arm/mach-armadaxp/Kconfig |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

--- a/arch/arm/mach-armadaxp/Kconfig
+++ b/arch/arm/mach-armadaxp/Kconfig
@@ -16,7 +16,7 @@ config ARMADA_XP
 choice
 	prompt "Armada XP Chip revision"
 	depends on ARMADA_XP
-	default ARMADA_XP_REV_Z1
+	default ARMADA_XP_REV_A0
 
 config  ARMADA_XP_REV_Z1
 	bool "MV88F78x30 and MV88F78x60 Z1 SoC devices"
