@@ -169,7 +169,7 @@ __setup("mv_port3_config=", mv_eth_cmdli
 struct mv_eth_nfp_ops *mv_eth_nfp_ops = NULL;
 int mv_eth_nfp_register(mv_eth_nfp_func_t *func)
 {
