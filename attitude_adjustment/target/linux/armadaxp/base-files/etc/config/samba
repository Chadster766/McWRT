
config samba
        option name 'McWrt'
        option workgroup 'WORKGROUP'
        option description 'McWrt'
        option homes '1'

config sambashare
        option name 'sda1'
        option path '/mnt/sda1'
        option read_only 'no'
        option guest_ok 'yes'
        option create_mask '0777'
        option dir_mask '0777'


