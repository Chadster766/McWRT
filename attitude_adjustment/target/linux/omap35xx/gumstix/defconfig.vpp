# defconfig for vpp

CONFIG_TARGET_omap35xx=y
CONFIG_TARGET_omap35xx_gumstix=y
CONFIG_TARGET_omap35xx_gumstix_Vpp=y
CONFIG_DEVEL=y
CONFIG_BROKEN=y
CONFIG_TOOLCHAINOPTS=y
CONFIG_GCC_VERSION_4_4_5=y
CONFIG_INSTALL_LIBSTDCPP=y
CONFIG_TARGET_OPTIONS=y
CONFIG_TARGET_OPTIMIZATION="-O2 -pipe -march=armv7-a -mtune=cortex-a8 -funit-at-a-time"
CONFIG_USE_EGLIBC=y
CONFIG_EGLIBC_VERSION_2_12=y
CONFIG_EGLIBC_REVISION="10454"
CONFIG_EGLIBC_VERSION="2.12"
CONFIG_LIBC="eglibc"
CONFIG_LIBC_VERSION="2.12"
CONFIG_TARGET_SUFFIX="gnueabi"
CONFIG_TARGET_ROOTFS_PARTSIZE=200
CONFIG_SOFT_FLOAT=n
CONFIG_TARGET_ROOTFS_EXT4FS=y
CONFIG_UDEV_EXTRA_firmware=y
CONFIG_BUSYBOX_CONFIG_STTY=y
CONFIG_BUSYBOX_CONFIG_TTY=y
CONFIG_TARGET_omap35xx_vpp=y
