/* color theme: white */
body
.progressbar-unfilled {
	color: Black;
	background-color: White;
}

#header,
#header #short-status,
#submenu,
#mainmenu,
#mainmenu li a,
#submenu li a,
#footer,
#save,
#save a,
#save .page-save,
#save .apply {
	color: #000;
}
body {
	background: #fff url(bg2.png) no-repeat scroll;
}

#content h3,
#mainmenu a:hover,
#submenu a:hover,
#mainmenu .selected a,
#submenu .selected a,
#save a:hover,
.progressbar-filled {
	background-color: #CCCCCC;
	color: Black;
}

#content h3.warning {
	color: Black;
	border-color: Black;
}
.odd {	background-color: #eee }
.warning { color: Black; }
