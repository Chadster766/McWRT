	# option 'src' '192.168.1.0/24'
	# option 'proto' 'udp'
	# option 'port_type' 'source-ports'
	# option 'ports' '5060,16384:16482'
	# option 'wanrule' 'wan'

config 'mwanfw'
	option 'src' '192.168.0.3'
	option 'proto' 'icmp'
	option 'wanrule' 'balancer'

config 'mwanfw'
	option 'dst' 'www.whatismyip.com'
	option 'wanrule' 'fastbalancer'
