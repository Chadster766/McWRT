	config PACKAGE_ndppd
		tristate "ndppd................................................... NDP Proxy Daemon"
		default y if DEFAULT_ndppd
		default m if ALL
		select PACKAGE_libc
		depends !(USE_LIBSTDCXX) || INSTALL_LIBSTDCPP
